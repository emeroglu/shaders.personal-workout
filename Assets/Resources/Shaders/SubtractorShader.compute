
#pragma kernel CSMain

RWTexture2D<float4> src;
RWTexture2D<float4> src2;
RWTexture2D<float4> res;

[numthreads(8,8,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    float4 color,color2,diff;

    color = src[id.xy];
    color2 = src2[id.xy];

    diff = color2 - color;
    diff.a = 1;

    res[id.xy] = diff;
} 